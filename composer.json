{
    "name": "silverstripe/data-layer",
    "description": "Consistent and flexible data capture for your website.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mojmir Fendek",
            "email": "mfendek@silverstripe.com"
        },
        {
            "name": "Bernie Hamlin",
            "email": "bernie@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\DataLayer\\": "src/",
            "SilverStripe\\DataLayer\\Tests\\": "tests/php/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
